                                      <b class="online">online</b>
                                    {{else}}
                                      <b class="offline">offline</b>
                                    {{/if}}
                                  </td>
                                  <td class="date">{{this.date}}</td>
                                  <td class="font-md value">
                                    {{#if this.onConnect}}
                                    {{loadValue this.temp ../this.option ../this.condition}}
                                    {{else}}
                                    <span class="font-yellow"><i class="icon-question-sign"></i></span>
                                    {{/if}}
