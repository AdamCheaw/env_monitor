<h2>Hi , {{session}}</h2>
<form action="/getData/subscribe" method="post">

  <div class="sensorData">
    {{#each items.data }}
      {{#if this.onConnect }}
          <div>Id: {{ this._id }}</div>
          <div>Date: {{ this.date }}</div>
          <div>Name: {{ this.name }}</div>
          <div>temp: {{ this.temp }}</div>
          <div>onConnect: {{ this.onConnect }}</div>
          <div><input type="checkbox" name="subscribe_checkbox" id="checkbox_{{ this._id }}" value="{{ this._id }}"> Subscribe</div>
          <div>------------------------------------</div>
      {{else}}
          <div><s>Id: {{ this._id }}</s></div>
          <div><s>Date: {{ this.date }}</s></div>
          <div><s>Name: {{ this.name }}</s></div>

          <!--<div><input type="checkbox" name="subscribe_checkbox" id="checkbox_{{ this._id }}" value="{{ this._id }}"> Subscribe</div>-->
          <div>------------------------------------</div>
      {{/if}}
    {{/each}}
  </div>
  <button type="submit">submit</button>
  <button> <a href="/observe">observe window</a></button>
</form>
