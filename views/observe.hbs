                                <th>Status</th>
                                <th>last updated</th>
                                <th>value</th>
                              </tr>
                              </thead>

                              <tbody>
                                {{#each this._sensorID}}
                                <tr id="table-row-{{this._id}}">
                                  <td>{{this.name}}</td>
                                  <td class="status">
