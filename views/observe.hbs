
{{>header}}
<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="span3">
        <div class="sidebar">
            <ul class="widget widget-menu unstyled">
              <li class="active"><a href="/getData"><i class="menu-icon icon-list-alt"></i>Sensors
              </a></li>
              <li><a href="task.html"><i class="menu-icon icon-tasks"></i>Tasks <b class="label orange pull-right">
                  19</b> </a></li>
              <li><a class="collapsed" data-toggle="collapse" href="#togglePages1"><i class="menu-icon icon-desktop">
                </i><i class="icon-chevron-down pull-right"></i><i class="icon-chevron-up pull-right">
                </i>observe window </a>
                  <ul id="togglePages1" class="collapse unstyled">
                      <li><a href="/observe"><i class="icon-table"></i> Table</a></li>
                      <li><a href="other-user-profile.html"><i class="icon-map-marker"></i> Graph</a></li>
                  </ul>
              </li>
            </ul>
            <!--/.widget-nav-->
        </div>
        <!--/.sidebar-->
      </div>
      <!--/.span3-->
      <div class="span9">
        <div class="content">
          <div class="module">
            <div class="module-head">

                <h3 style="float:left">Observer Window</h3>
                <img src="Edmin/images/Radio-200px.gif" style="height:25px;width:25px;margin:0px 5px">

            </div>
            <div class="module-body">

              <table class="table table-condensed table-striped datatable-1" id="sub-table">
                <thead>
                <tr>
                  <th>#</th>
                  <th>Name</th>
                  <th>Temparature</th>
                  <th>Status</th>
                  <th>last updated</th>
                  <th></th>
                </tr>
                </thead>
                <tbody id="tbody1">
                  {{#each items }}
                      {{#if this._sensorID.[0].onConnect }}
                      <tr id="{{this._sensorID.[0]._id}}">
                        <td class="sensor-index">{{@index}}</td>
                        <td>{{this._sensorID.[0].name}}</td>
                        <td class="sensor-temp">{{loadValue this._sensorID.[0].temp this.option this.condition}}</td>
                        <td class="sensor-status"><b class="online" >online</b></td>
                        <td class="sensor-date">{{this._sensorID.[0].date}}</td>
                        <td><a type="button"
                              class="btn-icon btn-danger unsubBtn"
                              id="unsubBtn-{{this._id}}"
                            >
                              <i class="icon-remove"></i>
                            </a>
                        </td>
                      </tr>
                      {{else}}
                      <tr id="{{this._sensorID.[0]._id}}">
                        <td class="sensor-index">{{@index}}</td>
                        <td>{{this._sensorID.[0].name}}</td>
                        <td class="sensor-temp">? </td>
                        <td class="sensor-status"><b class="offline" >offline</b></td>
                        <td class="sensor-date">{{this._sensorID.[0].date}}</td>
                        <td><a type="button"
                              class="btn-icon btn-danger unsubBtn"
                              id="unsubBtn-{{this._id}}"
                            >
                              <i class="icon-remove"></i>
                            </a>
                        </td>
                      </tr>
                      {{/if}}
                  {{/each}}
                </tbody>
              </table>
              <br>
              <div class="row-fluid mb-1">
                <div class="span12">
                    <div class="media user" style="border:1px solid #d9d9d9">
                        <div class="media-body">
                          <div class="pull-right">
                            <button type="button" name="" class="btn btn-mini btn-xs btn-success"><i class="media-icon icon-edit"></i></button>
                            <button type="button" name="" class="btn btn-mini btn-xs btn-danger"><i class="media-icon icon-remove"></i></button>
                          </div>
                          <div class="row-fluid">

                            <div class="span6">

