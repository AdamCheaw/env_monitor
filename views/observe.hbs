
{{>header}}
<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="span3">
        <div class="sidebar">
            <ul class="widget widget-menu unstyled">
              <li class="active"><a href="/getData"><i class="menu-icon icon-list-alt"></i>Sensors
              </a></li>
              <li><a href="task.html"><i class="menu-icon icon-tasks"></i>Tasks <b class="label orange pull-right">
                  19</b> </a></li>
              <li><a class="collapsed" data-toggle="collapse" href="#togglePages1"><i class="menu-icon icon-desktop">
                </i><i class="icon-chevron-down pull-right"></i><i class="icon-chevron-up pull-right">
                </i>observe window </a>
                  <ul id="togglePages1" class="collapse unstyled">
                      <li><a href="/observe"><i class="icon-table"></i> Table</a></li>
                      <li><a href="other-user-profile.html"><i class="icon-map-marker"></i> Graph</a></li>
                  </ul>
              </li>
            </ul>
            <!--/.widget-nav-->
        </div>
        <!--/.sidebar-->
      </div>
      <!--/.span3-->
      <div class="span9">
        <div class="content">
          <div class="module">
            <div class="module-head">
                <h3 style="float:left">Observer Window</h3>
                <img src="Edmin/images/Radio-200px.gif" style="height:25px;width:25px;margin:0px 5px">
            </div>
            <div class="module-body pb-5 " id="subscription-1">
              {{#each items }}
                {{#if this.groupType}}
                <div class="row-fluid">
                  <div class="span12">
                    <div class="media user" id="subscription-{{this._id}}" style="border-bottom:1px solid #d9d9d9;border-radius:3px;">
                      <div class="media-body">
                        <div class="pull-right">
                          <button type="button" class="btn btn-mini btn-xs editBtn" id="editBtn-{{this._id}}">
                            <i class="media-icon icon-edit"></i>
                          </button>
                          <button type="button" class="btn btn-mini btn-xs unsubBtn" id="unsubBtn-{{this._id}}">
                            <i class="media-icon icon-remove"></i>
                          </button>
                        </div>
                        <div class="row-fluid">
                          <div class="span10">
                            <h3 class="media-title mb-1">
                                {{title}}
                            </h3>
                            {{!-- <p><small class="muted">Lab B24 Outdoor</small></p> --}}
                            <p>option : {{this.option}}<br/>
                               grouping type : {{this.groupType}}<br/>
                            {{#if this.condition}}
                              condition : {{loadCondition this.condition}}
                                {{!-- {{#each this.condition}}
                                  - {{this.type}}<br/>
                                {{/each}} --}}
                            {{/if}}
                            {{!-- <strong>Group Type</strong>
						                 -
						                <span>{{this.groupType}}</span> --}}
                          </div>
                          <div class="span2">
                            <div class="pull-right">
                              <h3 class="sensor-value">{{{loadGroupValue this._sensorID this.groupType this.condition}}}</h3>
                            </div>
                          </div>

                        </div>
                        <div class="row-fluid">
                          <a class="btn btn-mini btn-collapse" data-toggle="collapse" href="#collapse-{{this._id}}"
                             role="button" aria-expanded="false" aria-controls="collapse-{{this._id}}">
                             <i class="icon-chevron-down"></i>
                          </a>
                          <div class="collapse multi-collapse" id="collapse-{{this._id}}">
                            <table class="table table-bordered">
                              <thead>
                              <tr>
                                <th>Name</th>
                                <th>Status</th>
                                <th>last updated</th>
                              </tr>
                              </thead>

                              <tbody>
                                {{#each this._sensorID}}
                                <tr id="table-row-{{this._id}}">
                                  <td>
                                    <a href="getData/{{this._id}}">
                                      {{this.name}}
                                    </a>
                                  </td>
                                  <td class="status">
                                    {{#if this.onConnect}}
                                      <b class="online">online</b>
                                    {{else}}
                                      <b class="offline">offline</b>
                                    {{/if}}
                                  </td>
                                  <td class="date">{{this.date}}</td>
                                </tr>
                                {{/each}}
                              </tbody>
                            </table>
                          </div>
                        </div>
                        {{!--end row-fluid --}}
                      </div>
                      {{!-- end media-body --}}
                    </div>
                  {{!-- end media --}}
                  </div>
                  {{!-- end span12 --}}
                </div>
                {{else}}
                  <div class="row-fluid">
                    <div class="span12">
                    <div class="media user" id="subscription-{{this._id}}" style="border-bottom:1px solid #d9d9d9;border-radius:3px;">
                      <div class="media-body">
                        <div class="pull-right">
                          <button type="button" class="btn btn-mini btn-xs editBtn"
                            id="editBtn-{{this._id}}">
                            <i class="media-icon icon-edit"></i>
                          </button>
                          <button type="button" class="btn btn-mini btn-xs unsubBtn"
                            id="unsubBtn-{{this._id}}">
                            <i class="media-icon icon-remove"></i>
                          </button>
                        </div>
                        <div class="row-fluid">
                        <div class="span10">
                          <h3 class="media-title mb-1">
                              <a href="getData/{{this._sensorID.[0]._id}}">{{this._sensorID.[0].name}}</a>
                          </h3>
                          {{!-- <p><small class="muted">Lorem ipsum dolor sit amet.</small></p> --}}
                          <p>option : {{this.option}}<br/>
                          {{#if this.condition}}
                            condition : {{loadCondition this.condition}}
                              {{!-- {{#each this.condition}}
                                - {{this.type}}<br/>
                              {{/each}} --}}
                          {{/if}}
                          </p>
                          {{#if this._sensorID.[0].onConnect}}
                          <b class="online">online</b>
                          {{else}}
                          <b class="offline">offline</b>
                          {{/if}}
                          <span class="ml-2">{{this._sensorID.[0].date}}</span>

                        </div><!-- end span6 -->
                        <div class="span2">
                          <div class="pull-right">
                            <h3 class="sensor-value">
                              {{#if this._sensorID.[0].onConnect}}
                              {{loadValue this._sensorID.[0].temp this.option this.condition}}
                              {{else}}
                              <span class="font-yellow"><i class="icon-question-sign"></i></span>
                              {{/if}}
                            </h3>
                          </div>

                        </div>
                      </div>
                      {{!--end row-fluid --}}
                    </div>
                    {{!-- end media-body --}}
                  </div>
                {{!-- end media --}}
              </div>
              {{!-- end span12 --}}
            </div>
            {{/if}}
            {{/each}}

            </div>
          </div>
        <br />

        </div><!--/.content-->
      </div><!--/.span9-->
    </div>
  </div>
</div>
{{>footer}}
{{>editForm}}
<script type="text/javascript">
  var name = "{{session}}";
  //var data = {{{subInfo}}};
</script>
<script src="/js/libs/handlebars-v4.0.12.min.js"></script>
<script src="/js/libs/moment.min.js"></script>
<script src="/js/editSubscription.js"></script>
<script src="/js/observe.js"></script>
